{
  "name": "vite",
  "private": true,
  "version": "0.0.1",
  "engines": {
    "node": ">=18 <19"
  },
  "type": "module",
  "scripts": {
    "dev": "vite --mode local-dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx . && prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@okta/okta-auth-js": "^7.0.1",
    "@okta/okta-react": "^6.7.0",
    "@sentry/react": "^7.17.4",
    "@sentry/tracing": "^7.17.4",
    "@tanstack/react-query": "^4.14.1",
    "@tanstack/react-query-devtools": "^4.14.1",
    "axios": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.24.5",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "rollup-plugin-visualizer": "^5.8.3",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.24.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,html,json,md,yml,yaml}": "prettier --write"
  }
}
